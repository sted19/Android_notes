<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.SwipeUp.utilities">
    <permission  android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/swipeicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/swipeicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name="com.SwipeUp.shuffleManagement.ShuffleActivity"
            android:label="Swipe Up"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.SwipeUp.swipeUpManagement.SwipeUpActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.SwipeUp.mainMenuManagement.MainMenuActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity android:name="com.SwipeUp.accessManagement.AccessActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name="com.SwipeUp.accessManagement.DataCollectionActivity"
            android:screenOrientation="portrait">

        </activity>

    </application>

</manifest>
